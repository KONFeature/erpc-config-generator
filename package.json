{
  "name": "erpc-config-generator",
  "description": "Simple typescript tools to generate eRPC yaml config file",
  "version": "0.0.1",
  "author": "KONFeature",
  "maintainers": [
    {
      "name": "Quentin Nivelais",
      "url": "https://twitter.com/QNivelais"
    }
  ],
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "typings": "dist/index.d.ts",
  "type": "module",
  "files": [
    "/dist"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.cjs"
    }
  },
  "peerDependencies": {
    "typescript": "^5.5.4",
    "viem": "^2.19.3"
  },
  "scripts": {
    "format": "biome check --write .",
    "format:check": "biome check .",
    "clean": "rimraf dist",
    "build": "tsup --splitting",
    "build:watch": "tsup --watch --splitting --format esm",
    "update-erpc-types": "bun run cli/generate-types.ts",
    "update:deps": "npm-check-updates -ui",
    "example": "bun run example/simple.ts && bun run example/frak.ts"
  },
  "devDependencies": {
    "@biomejs/biome": "1.8.3",
    "tsup": "^8.2.4",
    "@types/bun": "latest",
    "npm-check-updates": "^17.0.6"
  },
  "dependencies": {
    "yaml": "^2.5.0"
  }
}